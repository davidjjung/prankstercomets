{
	// Generates all block states for all blocks in the registry.
	"generate_all_known_states": true,
	// Whether blocks are replaced in existing chunks.
	"retro_gen": false,

	    "state_swapper": [
	    	{
	    		"new": {
	    			"Name": "immersive_weathering:environmental/cherry_leaf_pile",
	    			// Properties define the state of this block/fluid.
	    			"Properties": {
	    				"layers": 1
	    			}
	    		},
	    		"old": {
	    			"Name": "environmental:cherry_leaf_pile",
	    			// Properties define the state of this block/fluid.
	    			"Properties": {
	    				"down": true,
	    				"east": false,
	    				"north": false,
	    				"south": false,
	    				"up": false,
	    				"waterlogged": false,
	    				"west": false
	    			}
	    		}
	    	}
	    ],
	/* A map of blocks that specifies what the "old" block is and what its "new" block is.
	   Example:
	   	"swapper": {
	   		"minecraft:coarse_dirt": "minecraft:dirt",
	   		"minecraft:diamond_block": "minecraft:emerald_block"
	   	}
	*/
	"swapper": {"environmental:cherry_leaf_pile": "immersive_weathering:environmental/cherry_leaf_pile",
	"autumnity:orange_maple_leaf_pile": "immersive_weathering:autumnity/orange_maple_leaf_pile",
	"autumnity:maple_leaf_pile": "immersive_weathering:autumnity/maple_leaf_pile",
	"autumnity:red_maple_leaf_pile": "immersive_weathering:autumnity/red_maple_leaf_pile",
	"autumnity:yellow_maple_leaf_pile": "immersive_weathering:autumnity/yellow_maple_leaf_pile",
	}
}